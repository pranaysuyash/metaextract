{
  "name": "metaextract",
  "version": "4.0.0",
  "type": "module",
  "description": "The world's most comprehensive metadata extraction system - extracting a comprehensive universe of metadata fields from any file type across digital domains",
  "license": "MIT",
  "author": "MetaExtract Team",
  "homepage": "https://github.com/metaextract/metaextract#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/metaextract/metaextract.git"
  },
  "bugs": {
    "url": "https://github.com/metaextract/metaextract/issues"
  },
  "scripts": {
    "dev:client": "vite dev --port 5173",
    "dev:server": "PYTHON_EXECUTABLE=$PWD/.venv/bin/python3 NODE_ENV=development HOST=127.0.0.1 JWT_SECRET=your-super-secure-random-jwt-secret-here-at-least-32-characters-for-development node --import tsx/esm server/index.ts",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:tsx": "PYTHON_EXECUTABLE=$PWD/.venv/bin/python3 NODE_ENV=development tsx server/index.ts",
    "build": "tsx script/build.ts",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/index.cjs",
    "start:dev": "PYTHON_EXECUTABLE=$PWD/.venv/bin/python3 tsx watch server/index.ts",
    "check": "tsc",
    "check:watch": "tsc --watch",
    "db:push": "drizzle-kit push",
    "db:migrate": "node scripts/run_migrations.js",
    "seed:test-users": "tsx scripts/seed_test_users.ts",
    "db:studio": "drizzle-kit studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e:smoke": "playwright test tests/e2e/images-mvp.smoke.spec.ts",
    "test:e2e:visual": "playwright test tests/e2e --update-snapshots",
    "test:e2e:visual:ci": "playwright test tests/e2e --reporter=list",
    "smoke-server": "node scripts/smoke-server.js",
    "check:db": "node scripts/check_db.js",
    "db:init-debug": "node scripts/db-init-debug.cjs",
    "lint": "eslint client/src server shared --ext .ts,.tsx",
    "lint:fix": "eslint client/src server shared --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist && rm -rf client/dist",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run test:ci",
    "gate": "bash ./gate.sh",
    "prepare": "husky install"
  },
